@model List<MyBlog.EntityLayer.Concrete.Tag>
@{
    ViewData["Title"] = "Index";
    Layout = "~/Areas/Writer/Views/WriterLayout/Index.cshtml";
}

<div class="main-panel">
    <div class="content-wrapper">
        <div class="page-header">
            <h3 class="page-title">Tag Sayfası</h3>
            <nav aria-label="breadcrumb">
                <ol class="breadcrumb">
                    <li class="breadcrumb-item"><a href="#">Etiketler</a></li>
                    <li class="breadcrumb-item active" aria-current="page"> Etiketler </li>
                </ol>
            </nav>
        </div>
        <div class="row">
            <div class="col-lg-12 grid-margin stretch-card">
                <div class="card">
                    <div class="card-body">
                        <h4 class="card-title">Yorum Listesi</h4>
                        <p class="card-description">
                            
                        </p>
                        <div class="table-responsive">
                            <table class="table">
                                <thead>
                                    <tr>
                                        <th>Etiket Id</th>
                                        <th>Etiket İsmi</th>
                                       
                                    </tr>
                                </thead>
                                <tbody>
                                    @foreach (var item in Model)
                                    {
                                        <tr>
                                            <td>@item.TagId</td>
                                            <td>@item.TagTitle</td>
                                           
                                        </tr>
                                    }
                                </tbody>

                            </table>
                            <a href="/Admin/Tag/CreateTag/" class="btn btn-success-primary">Yeni Etilet Girişi</a>
                        </div>
                    </div>
                </div>
            </div>

        </div>
    </div>
</div>

